{
    "path": "/favicons/",

    "background": "#ffffff",
    "theme_color": "#ffffff",
    "appleStatusBarStyle": "default",

    "scope": "/#/app",
    "start_url": "/#/app",

    "display": "standalone",
    "orientation": "portrait",

    "logging": false,
    "html": "icons.html",
    "pipeHTML": true,
    "replace": true,

    "icons": {
        "android": {
            "background": "#ffffff",
            "offset": 10
        },
        "appleIcon": {
            "offset": 10
        },
        "appleStartup": {
            "offset": 15
        },
        "coast": false,
        "favicons": {
            "background": "#ffffff",
            "offset": 10
        },
        "firefox": {
            "offset": 15,
            "overlayGlow": false
        },
        "windows": {
            "background": "#ffffff",
            "offset": 10
        },
        "yandex": false
    }
}
